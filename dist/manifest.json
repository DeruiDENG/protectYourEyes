{
  "manifest_version": 2,

  "name": "Protect Your Eyes",
  "description": "Small extension to help keep your eyes healthy",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "static/popup.html"
  },

  "background": {
    "scripts": ["static/background.js"]
  },

  "permissions": [
    "notifications"
  ]
}
